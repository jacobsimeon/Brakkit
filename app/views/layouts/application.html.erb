<!DOCTYPE html>
<html>
<head>
  <title>Brakkit</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="wrapper">
    <%= session_link %>
    <p class="notice" ><%= notice %></p>
    <p class="alert" <%= alert %></p>
    <h1 id="brakkit-name">Brakkit</h1>
    <%= yield %>
  </div>
</body>
</html>
